    "allowJs": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "module": "es2022",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ESNext",
